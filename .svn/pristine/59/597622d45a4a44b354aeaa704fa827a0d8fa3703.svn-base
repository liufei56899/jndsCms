/* ========================================= */
.nav {
	height: 52px;
  background-color: #1d5b97;
  line-height: 52px;
  width: 100%;
  min-width: 1200px;
  position: static;
  z-index: 1000;
  margin-bottom: 15px;
}
.nav .nav-bar {
  width: 1200px;
  margin: 0 auto;
  height: 52px;
}
.nav .nav-bar li {
  float: left;
  width: 130px;
  position: relative;
}
.nav .nav-bar li a {
  display: block;
  font-size: 16px;
  text-align: center;
  color: #fff;
}
.nav .nav-bar li .active {
  background-color: #4b7dd8;
}
.nav .nav-bar li ul.erji {
  position: absolute;
  z-index: 10;
}
.nav .nav-bar li ul.erji li {
  background-color: #4b7dd8;
  /*border-bottom: 1px solid #ffffff;*/
  width: 130px;
}
.nav .nav-bar li ul.erji li a:hover {
  background-color: #1d5b97;
}














.banner-slider {
  border-bottom: 5px solid #fb565c;
  height: 405px;
}
.banner-slider .scroll {
  margin: 0 auto;
  width: 100%;
  height: 400px;
  position: relative;
  overflow: hidden;
}
.banner-slider .scroll .scroll-ul {
  list-style-type: none;
  padding: 0;
  margin: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 99999px;
  height: 400px;
}
.banner-slider .scroll .scroll-li {
  float: left;
  width: 1920px;
}
.banner-slider .scroll .scroll-li > img {
  width: 1920px;
  height: 400px;
}
.banner-slider .prev_ {
  position: absolute;
  left: 30px;
  top: 168px;
}
.banner-slider .next_ {
  position: absolute;
  right: 30px;
  top: 168px;
}
