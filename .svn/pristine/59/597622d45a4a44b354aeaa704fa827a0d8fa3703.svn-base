}
.contest .contest-documents .contest-documents-cont {
  width: 375px;
}
.contest .contest-documents .contest-documents-cont ul {
  line-height: 36px;
}
.contest .contest-documents .contest-documents-cont ul li {
  background: url("../img/dian.png") no-repeat 9px 12px;
  padding-left: 23px;
  font-size: 16px;
  border-bottom: 1px dashed #dedede;
}
.contest .contest-documents .contest-documents-cont ul li a:hover {
  color: #1d5b97;
}
.contest .contest-documents .contest-img-cont {
  width: 375px;
}
.contest .contest-documents .contest-img-cont ul li {
  width: 100px;
  float: left;
  margin-right: 37.5px;
  margin-bottom: 5px;
}
.contest .contest-documents .contest-img-cont ul li img {
  width: 100px;
  height: 70px;
}
.contest .contest-documents .contest-img-cont ul li .img-title {
  width: 100px;
  text-align: center;
  display: block;
  font-size: 14px;
}
.contest .contest-documents .contest-img-cont ul li:last-child,
.contest .contest-documents .contest-img-cont ul li:nth-child(3) {
  margin-right: 0;
}
